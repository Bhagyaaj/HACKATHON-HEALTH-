void fish()
{   printf("Enter the type of fish consumed by choosing the number against it\n");
    L:
    printf("\t FISHES");
    printf("\n1.Salmon\n2.Tuna\n3.Crustaceans(Shrimp,Prawns,Lobster,Crabs)\n4.Tilapa\n5.Catfish\n6.Shellfish(Calms,Oysters,Mussels,Scallops)\n7.Sardines\n8.Freshwater Fishes\n9.Exit\n");
    scanf("%d",&x);
    switch(x)
    {  case 1:Salmon();goto L;
       case 2:Tuna();goto L;
       case 3:Crust();goto L;
       case 4:Tilapa();goto L;
       case 5:Catfish();goto L;
       case 6:Shellfish();goto L;
       case 7:Sardines();goto L;
       case 8:Freshwater();goto L;
       case 9:break;
    }
    printf("Total calories got from fish today:%lf\n",cal1);
}
