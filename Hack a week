void grampulses()
{   printf("Enter the type of grain consumed by choosing the number against it\n");
    L:
    printf("\n1.Dry beans\n2.Lentils\n3.Chick peas\n4.Lupins5\n.Dry peas\n6.Green gram\n7.Black gram\n8.Exit\n");
    scanf("%d",&x);
    switch(x)
    {  case 1:dry_beans();goto L;
       case 2:lentils();goto L;
       case 3:chick_peas();goto L;
       case 4:lupins();goto L;
       case 5:dry_peas();goto L;
       case 6:green_gram();goto L;
       case 7:black_gram();goto L;
       case 8:break;
    }
    printf("Total calories got from pulses and grams today:%lf",cal7);
}
    
void main()
{   printf("Enter the food item consumed by choosing the number against it\n");
    L:
    printf("\t FOOD ITEMS");
    printf("\n1.FISH\n2.MEAT\n3.FRUITS\n4.VEGETABLES\n5.DAIRY PRODUCTS\n6.GRAINS\n7.GRAMS AND PULSES\n8.EXIT\n");
    scanf("%d",&x);
    switch(x)
    {  case 1:fish();goto L;
       case 2:meat();goto L;
       case 3:fruits();goto L;
       case 4:vegetables();goto L;
       case 5:dairy();goto L;
       case 6:grains();goto L;
       case 7:grampulses();goto L;
       case 8:break;
    }
    printf("Total calories got today(overall):%lf\n",cal1+cal2+cal3+cal4+cal5+cal6+cal7);
    if(cal1==0.00)
    printf("You haven't consumed fish today!!\nChances of vitamin anf fat deficiency\n");
    if(cal2==0.00)
    printf("You haven't consumed meat today!!\nChances of protein deficiency\n");
    if(cal3==0.00)
    printf("You haven't consumed fruits today!!\nChances of vitamin,antioxidants and fibre deficiency\n");
    if(cal4==0.00)
    printf("You haven't consumed vegetables today!!\nChances of potassium,fibre and vitamin deficiency\n");
    if(cal5==0.00)
    printf("You haven't consumed dairy today!!\nChances of calcium,vitamin(B2,B12) deficiency\n");
    if(cal6==0.00)
    printf("You haven't consumed grains today!!\nChances of carbohydrate(mainly),fibre,vitamin and mineral deficiency\n");
    if(cal7==0.00)
    printf("You haven't consumed grams and pulses today!!\nChances of protein and fibre deficiency\n");
    if(cal1!=0.00&&cal2!=0.00&&cal3!=0.00&&cal4!=0.00&&cal5!=0.00&&cal6!=0.00&&cal7&&(cal1+cal2+cal3+cal4+cal5+cal6+cal7)>=2200)
    printf("CONGRATS!!You had a complete balanced diet today\nGOOD HEALTH WISHES\n");
    if(cal1!=0.00&&cal2!=0.00&&cal3!=0.00&&cal4!=0.00&&cal5!=0.00&&cal6!=0.00&&cal7&&(cal1+cal2+cal3+cal4+cal5+cal6+cal7)<2200)
    printf("You had included all food items but the amount doesn't meet the calorie requirements!!\nEAT MORE\n");
    
}
